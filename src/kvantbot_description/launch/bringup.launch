<launch>
	<param name="robot_description" command="$(find xacro)/xacro '$(find kvantbot_description)/urdf/Robot.xacro' --inorder"/>
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen" />

	<include file="$(find kvantbot_base)/launch/kvantbot_base.launch" />
	<include file="$(find kvantbot_control)/launch/kvantbot_control.launch" />
	<include file="$(find kvantbot_driver)/launch/kvantbot_drivers.launch" />
	<include file="$(find kvantbot_teleop)/launch/kvantbot_teleop.launch" />
</launch>